<Window x:Class="Dzik.PlaintextWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dzik"
        mc:Ignorable="d"
        Title="Tekst odszyfrowany" Height="322" Width="514" Topmost="True" WindowStartupLocation="CenterOwner">
    <Grid>
        <TextBox x:Name="Output" Margin="0,42,0,63" TextWrapping="Wrap" Text="" IsReadOnly="True" IsReadOnlyCaretVisible="True" Style="{DynamicResource InputTextBoxStyle}"/>
        <Button x:Name="CloseWindowButton" Content="Zamknij" Margin="0,0,10,10" HorizontalAlignment="Right" Width="112" Height="48" VerticalAlignment="Bottom" Click="CloseWindowButton_Click"/>

    </Grid>
</Window>
